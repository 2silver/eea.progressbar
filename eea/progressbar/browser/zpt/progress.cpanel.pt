<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/prefs_main_template/macros/master"
      i18n:domain="eea">

<body>
<div id="content" metal:fill-slot="prefs_configlet_main">
  <div id="eea-progressbar-content">
    <tal:field tal:repeat="field view/schema">
      <fieldset tal:attributes="id string:progress-schema-${field/getName}">
        <legend tal:content="python:field.widget.label">Field label</legend>
      </fieldset>
    </tal:field>
  </div>
</div>
</body>
</html>
